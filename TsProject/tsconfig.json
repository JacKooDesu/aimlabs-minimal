{
  "files": [
    "../Assets/Gen/Typing/csharp/index.d.ts"
  ],
  "include": [
    "./src/*",
    "./src/**/*.ts",
    "./src/**/*.mts",
    "./types/**.d.ts",
    "../Assets/PuerTS/Typing",
    "../Assets/Gen/Typing/",
    "../Assets/Gen/Typing/csharp/"
  ],
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "lib": [
      "DOM",
      "ES2020",
      "ScriptHost",
      "DOM.Iterable"
    ],
    "types": [],
    "preserveSymlinks": true,
    "allowJs": true,
    "checkJs": false,
    "outDir": "./output",
    "incremental": true,
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": false,
    "strict": false,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "stripInternal": false
  }
}